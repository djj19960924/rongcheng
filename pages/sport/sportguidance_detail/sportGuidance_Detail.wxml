<!-- header -->
<h_header class='box_top' title='{{title}}'></h_header>

<view class='title'>
  <image class='left' src='../images/{{iconName}}.png' mode='widthFix'></image>
  <view class='right'>
    <view class='c_title rightTop'>{{activeType}}</view>
    <view class='c_content rightBottom'>{{exerciseName}}</view>
  </view>
</view>

<view class='content'>
  <view class='contentTop c_title'>
    <image class='icon' src='../images/ex1.png'></image>
    锻炼要点
  </view>
  <view class='contentBottom c_sub'>
  {{exercisePoint}}
     </view>
</view>

<view class='content'>
  <view class='contentTop c_title'>
    <image class='icon' src='../images/ex2.png'></image>
    锻炼强度
  </view>
  <view class='contentBottom c_sub'>
    {{intensity}}
  </view>
</view>

<view style='border:0' class='content'>
  <view class='contentTop c_title'>
    <image class='icon' src='../images/ex3.png'></image>
    锻炼频率
  </view>
  <view class='pinlv c_sub'>
    每周{{frequencyL}} - {{frequencyH}}次
  </view>
  <view class='pinlv c_sub'>
    {{suggestTimes}}
  </view>
  <view style='padding-bottom:20rpx;' class='pinlv c_sub'>
    {{notice}}
  </view>
</view>